graph title {
    node[shape=circle fixedsize=true style=filled fillcolor=white colorscheme=accent8 ];
    edge[style="solid",color="black",arrowhead="forward" colorscheme=accent8];
    1--2;
    1--3;
    2--4[penwidth=4, color=6];
    2--5[penwidth=4, color=6];
    3--6;
    3--7;
    {
        rank=same;
        2--x--3[style=invis];
    }
    x[style=invis];
}
